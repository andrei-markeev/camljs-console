{
	"name": "CamlJS Console",
    "homepage_url": "https://github.com/andrei-markeev/camljs-console",
	"version": "1.0.3",
	"description": "CamlJS Console with live preview.",
	"icons": {
        "16": "AppIcon16.png",
        "32": "AppIcon32.png",
        "48": "AppIcon48.png",
        "96": "AppIcon96.png",
        "128": "AppIcon.png"
	},
    "browser_action": {
        "default_icon": {
            "19": "AppIcon19.png",
            "38": "AppIcon38.png"
        },
        "default_popup": "index.html"
    },
    "permissions":  [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"manifest_version": 2
}